function mesesDoAno(m) {
switch(m){
case 1: m = "Janeiro"
break;
case 2: m = "Fevereiro"
break;
case 3: m = "Março"
break;
case 4: m = "Abril"
break;
case 5: m = "Maio"
break;
case 6: m = "Junho"
break;
case 7: m = "Julho"
break;
case 8: m = "Agosto"
break;
case 9: m = "Setembro"
break;
case 10: m = "Outubro"
break;
case 11: m = "Novembro"
break;
case 12: m = "Dezembro"
break;
default: m = "Número inválido. Por favor insira um número entre 1 a 12."
}
return m
}
